{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "dart.flutterSdkPath": ".fvm/versions/3.19.5",
  "search.exclude": {
    "**/.fvm": true,
    "**/.g.dart": true,
    "**/.freezed.dart": true
  },
  "files.watcherExclude": {
    "**/.fvm": true,
    "**/.g.dart": true,
    "**/.freezed.dart": true
  },
  "dart.lineLength": 120,
  "[dart]": {
    "editor.rulers": [
      120
    ]
  }
}